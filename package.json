{
  "name": "@fwryan/content-tools",
  "version": "2.0.0",
  "description": "Tools and utilities for working with FirstWho as a headless CMS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstwho/content-tools.git"
  },
  "author": "Ryan Mahoney",
  "license": "MIT",
  "homepage": "https://firstwho.co",
  "keywords": [],
  "files": [
    "dist",
    "fetch.sh"
  ],
  "main": "dist/index.js",
  "bin": {
    "get-content": "src/get-content.js"
  },
  "scripts": {
    "build": "rm -rf ./dist && babel src --out-dir dist --extensions \".js,.jsx\"",
    "test": "mocha --require @babel/register"
  },
  "browserslist": "> 0.25%, not dead",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/node": "^7.29.0",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/register": "^7.28.6",
    "assert": "^2.1.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-transform-es2015-modules-simple-commonjs": "^0.3.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-watch": "^7.8.1",
    "css-loader": "^7.1.3",
    "global-jsdom": "^28.0.0",
    "jsdom": "^28.0.0",
    "mocha": "^11.7.5"
  },
  "dependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.7.7",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.1.0",
    "@atlaskit/pragmatic-drag-and-drop-live-region": "^1.3.3",
    "@headlessui/react": "^2.2.9",
    "copy-to-clipboard": "^3.3.3",
    "core-js": "^3.48.0",
    "react-hot-toast": "^2.6.0",
    "react-scroll-into-view": "^2.2.4"
  },
  "peerDependencies": {
    "adm-zip": "^0.5.16",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  }
}
